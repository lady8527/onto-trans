ontology:lexinfo
	a voaf:Vocabulary ,
	  owl:NamedIndividual ,
	  owl:Ontology ;
	ns3:license by: ;
	dcterms:contributor ae: ,
			    <http://www.cimiano.de/philipp> ,
			    <http://www.paulbuitelaar.net/> ;
	dcterms:creator ae: ;
	dcterms:description "An ontology of types, values and properties to be used with the lemon model, partially derived from ISOcat."@en ,
			    "Онтология типов, значений и свойств, используемых совместно с онтологией lemon, частично является производной от ISOcat."@ru ;
	dcterms:issued "2010-11-22"^^xsd:date ;
	dcterms:modified "2016-07-13"^^xsd:date ;
	dcterms:publisher ae: ;
	dcterms:rights by: ,
		       "Copyright 2010 Monnet Project"@en ;
	dcterms:title "LexInfo"@en ;
	vann:preferredNamespacePrefix "lexinfo" ;
	vann:preferredNamespaceUri "http://www.lexinfo.net/ontology/2.0/lexinfo#"^^xsd:anyURI ;
	rdfs:comment "An ontology of types, values and properties to be used with the lemon model, partially derived from ISOcat."@en ,
		     ""@ru ;
	rdfs:label "LexInfo"@en ,
		   ""@ru ;
	owl:imports <http://lemon-model.net/lemon> ;
	owl:versionInfo "2.0"@en .

lexinfo:AbbreviatedForm
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Сокращенная форма"@ru ;
	rdfs:subClassOf lexinfo:TermType .

lexinfo:AccusativePostPositiveArgument
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PostPositiveArgument ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:PostPositiveArgument
						   [ a owl:Restriction ;
						     owl:hasValue lexinfo:accusativeCase ;
						     owl:onProperty lemon:marker
						   ]
						 )
			    ] .

lexinfo:AdjectivalComplementFrame
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:VerbFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:VerbFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:predicativeAdjective
						   ]
						 )
			    ] .

lexinfo:Adjective
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Прилагательное"@ru ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:allValuesFrom lexinfo:AdjectiveFrame ;
			  owl:onProperty lemon:synBehavior
			] ,
			lemon:Word ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:AdjectivePOS
			    ] .

lexinfo:AdjectiveAccusativePostPositiveFrame
	a owl:Class ;
	lexinfo:example "wird wurden unseren irrtum GEWAHR"@de ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectivePostPositiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectivePostPositiveFrame
						   [ a owl:Restriction ;
						     owl:onProperty lexinfo:postPositiveArg ;
						     owl:someValuesFrom lexinfo:AccusativePostPositiveArgument
						   ]
						 )
			    ] .

lexinfo:AdjectiveAttributiveFrame
	a owl:Class ;
	lexinfo:example "the RED ball"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:attributiveArg
						   ]
						 )
			    ] .

lexinfo:AdjectiveComparativeFrame
	a owl:Class ;
	lexinfo:example "new york is BIGGER THAN berlin"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:comparativeAdjunct
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeSubject
						   ]
						 )
			    ] .

lexinfo:AdjectiveDativePostPositiveFrame
	a owl:Class ;
	lexinfo:example "er ist seinem bruder ÄHNLICH"@de ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectivePostPositiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectivePostPositiveFrame
						   [ a owl:Restriction ;
						     owl:onProperty lexinfo:postPositiveArg ;
						     owl:someValuesFrom lexinfo:DativePostPositiveArgument
						   ]
						 )
			    ] .

lexinfo:AdjectiveFrame
	a owl:Class ;
	rdfs:subClassOf lemon:Frame .

lexinfo:AdjectiveGenitivePostPositiveFrame
	a owl:Class ;
	lexinfo:example "er ist des hochverrats SCHULDIG"@de ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectivePostPositiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectivePostPositiveFrame
						   [ a owl:Restriction ;
						     owl:onProperty lexinfo:postPositiveArg ;
						     owl:someValuesFrom lexinfo:GenitivePostPositiveArgument
						   ]
						 )
			    ] .

lexinfo:AdjectiveImpersonalFrame
	a owl:Class ;
	lexinfo:example "es ist mir HEISS"@de ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:object
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "0"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeSubject
						   ]
						 )
			    ] .

lexinfo:AdjectivePOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PartOfSpeech .

lexinfo:AdjectivePPFrame
	a owl:Class ;
	lexinfo:example "he is RELATED TO her"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectivePredicativeFrame
						   lexinfo:PrepositionalFrame
						 )
			    ] .

lexinfo:AdjectivePhrase
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lemon:Phrase ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lemon:Phrase
						   [ a owl:Restriction ;
						     owl:onProperty lexinfo:head ;
						     owl:someValuesFrom lexinfo:Adjective
						   ]
						 )
			    ] .

lexinfo:AdjectivePostPositiveFrame
	a owl:Class ;
	lexinfo:example "that is something INTERESTING"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeArg
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:postPositiveArg
						   ]
						 )
			    ] .

lexinfo:AdjectivePredicateFrame
	a owl:Class ;
	rdfs:comment "Note this frame should be used when both attributive and predicative use of the adjective is allowed"@en ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveAttributiveFrame
						   lexinfo:AdjectivePredicativeFrame
						 )
			    ] .

lexinfo:AdjectivePredicativeFrame
	a owl:Class ;
	lexinfo:example "he is HAPPY"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeSubject
						   ]
						 )
			    ] .

lexinfo:AdjectiveScaleFrame
	a owl:Class ;
	lexinfo:example "Everest is 8,848m HIGH"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:adverbialComplement
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeSubject
						   ]
						 )
			    ] .

lexinfo:AdjectiveSuperlativeFrame
	a owl:Class ;
	lexinfo:example "tokyo is the BIGGEST of all metropoles"@en ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:AdjectiveFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:AdjectiveFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:superlativeAdjunct
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:copulativeSubject
						   ]
						 )
			    ] .

lexinfo:Adjunct
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:hasValue true ;
			  owl:onProperty lemon:optional
			] ,
			lemon:Argument .

lexinfo:Adposition
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Прелог или послелог"@ru ;
	rdfs:subClassOf lemon:Word ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:AdpositionPOS
			    ] .

lexinfo:AdpositionPOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PartOfSpeech .

lexinfo:AdpositionalObject
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf [ a owl:Restriction ;
			  owl:onProperty lemon:marker ;
			  owl:someValuesFrom lexinfo:Adposition
			] ,
			lexinfo:Object ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:Object
						   [ a owl:Restriction ;
						     owl:onProperty lemon:marker ;
						     owl:someValuesFrom lexinfo:Adposition
						   ]
						 )
			    ] .

lexinfo:Adverb
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Наречие"@ru ;
	rdfs:subClassOf lemon:Word ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:AdverbPOS
			    ] .

lexinfo:AdverbPOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PartOfSpeech .

lexinfo:AdverbialComplementFrame
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:VerbFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:VerbFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:predicativeAdverb
						   ]
						 )
			    ] .

lexinfo:Affix
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Аффикс"@ru ;
	rdfs:subClassOf lemon:Part ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:hasValue lexinfo:affix ;
			      owl:onProperty lexinfo:termElement
			    ] .

lexinfo:Animacy
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Одушевленность"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:ArbitraryControl
	a owl:Class ;
	rdfs:comment "Indicates either the subject or object of the main clause may be the omitted argument of the subclause"@en ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:Control .

lexinfo:Article
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Артикль"@ru ;
	rdfs:subClassOf lexinfo:Determiner ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:ArticlePOS
			    ] .

lexinfo:ArticlePOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:DeterminerPOS .

lexinfo:Aspect
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Вид"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:AttributiveArgument
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lemon:Argument .

lexinfo:CJK_compound
	a lexinfo:TermType ,
	  owl:NamedIndividual ,
	  owl:Thing ;
	ns2:creator "Wright, Sue Ellen" ;
	ns1:datcat datcat:DC-356 ;
	rdfs:comment "A lexical unit in a CJKV language that is represented by at least two CJKV characters." ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "compound(cjkv)" ,
		   "словосочетание в CJKV"@ru ;
	owl:versionInfo "1:0" .

lexinfo:Case
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Падеж"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:ClausalArgument
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lemon:Argument .

lexinfo:Cliticness
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Клитика"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:CommonNoun
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Имя нарицательное"@ru ;
	rdfs:subClassOf lexinfo:Noun ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:hasValue lexinfo:commonNoun ;
			      owl:onProperty lexinfo:partOfSpeech
			    ] .

lexinfo:ComparativeAdjunct
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:Adjunct .

lexinfo:Complement
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lemon:Argument .

lexinfo:Conjunction
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Союз"@ru ;
	rdfs:subClassOf lemon:Word ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:ConjunctionPOS
			    ] .

lexinfo:ConjunctionPOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PartOfSpeech .

lexinfo:Control
	a owl:Class ;
	rdfs:comment "Control indicates how a an argument from a main clause will be utilized in a subclause. This class includes both control structures and raising structures"@en ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lemon:Frame .

lexinfo:Dating
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Датирование"@ru ;
	rdfs:subClassOf lemon:PropertyValue .

lexinfo:DativePostPositiveArgument
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PostPositiveArgument ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:PostPositiveArgument
						   [ a owl:Restriction ;
						     owl:hasValue lexinfo:dativeCase ;
						     owl:onProperty lemon:marker
						   ]
						 )
			    ] .

lexinfo:DativeTransitiveFrame
	a owl:Class ;
	lexinfo:example "ich DANKE dir"@de ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:VerbFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:VerbFrame
						   [ a owl:Restriction ;
						     owl:cardinality "0"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:directObject
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:indirectObject
						   ]
						   [ a owl:Restriction ;
						     owl:cardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:subject
						   ]
						 )
			    ] .

lexinfo:DeclarativeClause
	a owl:Class ;
	rdfs:comment "A clause beginning with the declarative conjunction (\"that\" in English)"@en ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:ClausalArgument .

lexinfo:DeclarativeFrame
	a owl:Class ;
	rdfs:comment "Declarative frames take a clause as an argument, this argument is marker with the declarative conjunction (\"that\" in English)"@en ,
		     ""@ru ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:VerbFrame ;
	owl:equivalentClass [ a owl:Class ;
			      owl:intersectionOf ( lexinfo:VerbFrame
						   [ a owl:Restriction ;
						     owl:minCardinality "1"^^xsd:nonNegativeInteger ;
						     owl:onProperty lexinfo:declarativeClause
						   ]
						 )
			    ] .

lexinfo:Definiteness
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Определенность"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:Degree
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Степень"@ru ;
	rdfs:subClassOf lexinfo:MorphosyntacticProperty .

lexinfo:Determiner
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:label "Детерминанта"@ru ;
	rdfs:subClassOf lemon:Word ;
	owl:equivalentClass [ a owl:Restriction ;
			      owl:onProperty lexinfo:partOfSpeech ;
			      owl:someValuesFrom lexinfo:DeterminerPOS
			    ] .

lexinfo:DeterminerPOS
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
	rdfs:subClassOf lexinfo:PartOfSpeech .

lexinfo:DirectObject
	a owl:Class ;
	rdfs:isDefinedBy ontology:lexinfo ;
